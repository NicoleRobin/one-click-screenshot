{
  "manifest_version": 3,
  "name": "One click screen shot",
  "description": "Take screen shot of whole chrome page by one click. ", 
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png"
  }, 
  "action": {
    "default_title": "One click screen shot!"
  }, 
  "permissions": [
    "contextMenus", 
    "activeTab", 
    "downloads"
  ],
  "background": {
    "service_worker": "service_worker.js"
  } 
}
